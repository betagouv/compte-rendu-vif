{
  "name": "@cr-vif/frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm icons && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "pnpm env:gen && vite preview --host",
    "env:gen": "vite-node ./scripts/createEnvFile.ts",
    "prepare": "pnpm dsfr && panda codegen",
    "dsfr": "copy-dsfr-to-public && pnpm panda-ds",
    "icons": "only-include-used-icons",
    "panda-ds": "vite-node ./scripts/generatePandaDS.ts",
    "panda-ds:dev": "vite-node --watch ./scripts/generatePandaDS.ts",
    "client:generate": "pnpm electric-sql generate && pnpm client:complete",
    "client:complete": "vite-node ./scripts/completePrismaClient.ts"
  },
  "dependencies": {
    "@ark-ui/anatomy": "^2.3.1",
    "@ark-ui/react": "^2.2.3",
    "@codegouvfr/react-dsfr": "^1.9.2",
    "@cr-vif/electric-client": "workspace:*",
    "@emotion/react": "^11.11.4",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@gouvfr/dsfr-chart": "^1.0.0",
    "@mui/material": "^5.15.15",
    "@prisma/client": "^4.8.1",
    "@react-pdf/renderer": "^3.4.2",
    "@tanstack/react-query": "^4.18.0",
    "@tanstack/react-router": "^1.22.2",
    "@tanstack/router-devtools": "^1.22.2",
    "@tiptap/extension-link": "^2.3.0",
    "@tiptap/extension-placeholder": "^2.3.0",
    "@tiptap/react": "^2.3.0",
    "@tiptap/starter-kit": "^2.3.0",
    "date-fns": "^3.6.0",
    "electric-sql": "^0.10.1",
    "install": "^0.13.0",
    "npm": "^10.5.0",
    "ofetch": "^1.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-hook-form": "^7.51.1",
    "react-icons": "^5.1.0",
    "react-pdf-html": "^2.0.4",
    "react-use": "^17.5.0",
    "uuid": "^9.0.1",
    "vite-plugin-wasm": "^3.3.0",
    "wa-sqlite": "github:rhashimoto/wa-sqlite",
    "zod": "^3.22.4"
  },
  "imports": {
    "#styled-system/*": "./styled-system/*"
  },
  "devDependencies": {
    "@pandabox/unplugin": "^0.0.1",
    "@pandacss/dev": "^0.36.1",
    "@park-ui/panda-preset": "^0.36.1",
    "@tanstack/router-vite-plugin": "^1.20.5",
    "@types/css-tree": "^2.3.7",
    "@types/node": "^20.11.28",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "css-tree": "^2.3.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.38",
    "prisma": "^4.8.1",
    "typescript": "^5.4",
    "vite": "^5.1.6",
    "vite-node": "^1.4.0"
  }
}
